   "source": [
    "# Refactoring the temperature conversion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def fahrenheit_to_celsius(temp):\n",
    "    CONST, RATIO = 32, 5/9\n",
    "\n",
    "    return round((temp - CONST) * RATIO, 2)\n",
    "\n",
    "\n",
    "def celsius_to_fahrenheit(temp):\n",
    "    CONST, RATIO = 32, 5/9\n",
    "\n",
    "    return round((temp/RATIO) + CONST, 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
